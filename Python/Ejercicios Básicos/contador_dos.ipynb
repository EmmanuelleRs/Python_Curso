    "def contar_letras(cadena):\n",
    "    contador = {}\n",
    "    for letra in cadena:\n",
    "        if letra in contador:\n",
    "            contador[letra] += 1\n",
    "        else:\n",
    "            contador[letra] = 1\n",
    "    return contador"
   ]
